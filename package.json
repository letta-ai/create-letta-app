{
  "name": "create-letta-app",
  "description": "A new CLI generated with oclif",
  "version": "0.0.8",
  "author": "Letta",
  "bin": {
    "create-letta-app": "./bin/run.js"
  },
  "bugs": "https://github.com/letta-ai/create-letta-app/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.4.1",
    "@letta-ai/letta-client": "^0.1.93",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ink": "^5.2.0",
    "inquirer": "^12.5.2",
    "inquirer-autocomplete-standalone": "^0.8.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/react": "^19.1.1",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/letta-ai/create-letta-app",
  "keywords": [
    "oclif",
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "create-letta-app",
    "dirname": "create-letta-app",
    "commands": {
      "strategy": "single",
      "target": "./dist/commands/generate.js"
    },
    "topicSeparator": " ",
    "topics": {
      "generate": {
        "description": "Generate a new Letta project"
      }
    }
  },
  "repository": "letta-ai/create-letta-app",
  "scripts": {
    "dev": "shx rm -rf dist && tsc && cp -r ./example-apps ./dist && NODE_ENV=create-letta-dev create-letta-app",
    "build": "shx rm -rf dist && tsc -b && cp -r ./example-apps ./dist",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
